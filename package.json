{
  "name": "moonbit-registry",
  "version": "0.1.0",
  "description": "CLI tool for managing MoonBit registries",
  "type": "module",
  "main": "src/index.ts",
  "bin": {
    "moonbit-registry": "dist/moonbit-registry"
  },
  "scripts": {
    "build": "bun build src/index.ts --compile --outfile dist/moonbit-registry",
    "dev": "bun --watch src/index.ts",
    "test": "bun test",
    "lint": "biome check .",
    "fmt": "biome format --write ."
  },
  "dependencies": {
    "commander": "^12.0.0",
    "toml": "^3.0.0"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.0",
    "@types/bun": "latest",
    "typescript": "^5.0.0"
  }
}
